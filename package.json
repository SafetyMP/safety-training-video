    "react-dom": "^18.2.0",
    "replicate": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20",
